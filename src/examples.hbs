<!DOCTYPE html>
<html lang="en">
  {{> header isDataAtWork=true}}
  <p class="lede">Usage examples for all datasets listed in the <a href="/">Registry of Open Data on AWS</a>{{#if tag}} tagged with <strong>{{tag}}</strong>{{/if}}.</p>
  <hr>

  {{#each datasets}}
    {{#if DataAtWork}}
      <div id="{{Slug}}" class="dataset">
        <h4><a href="../{{Slug}}/">{{Name}}</a></h4>
        <ul class="dataatwork-list">
        {{#each DataAtWork}}
          <li>
            <a href="{{URL}}" class="{{#isAWSURL URL}}aws-link{{/isAWSURL}}" target="_none">{{Title}}</a> by {{#if AuthorURL}}<a href="{{AuthorURL}}" target="_none">{{AuthorName}}</a>{{else}}{{AuthorName}}{{/if}}</li></a>
          </li>
        {{/each}}
        </ul>
      </div>
    {{/if}}
  {{/each}}

  {{> footer isExamples=true}}